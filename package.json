{
  "name": "react-autonumeric",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run compile && vite build",
    "compile": "tsc --build . src/lib src/test src/demo",
    "coverage": "vitest run --coverage --typecheck",
    "format": "eslint --fix . && prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 && prettier . --check",
    "preview": "vite preview",
    "test": "vitest --typecheck",
    "test-once": "npm run coverage",
    "test-all": "npm run lint && npm run compile && npm run test-once"
  },
  "dependencies": {
    "@8hobbies/utils": "^1.2.1",
    "autonumeric": "^4.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@8hobbies/eslint-conf-baseline": "^2.1.0",
    "@8hobbies/test-utils": "^1.1.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "vite": "^5.2.9",
    "vitest": "^1.4.0"
  }
}
